<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!-- Font Awesome -->
    @Styles.Render("~/AdminLTE3-master/plugins/fontawesome-free/css/all.min.css")
    <!-- Ionicons -->
    @Styles.Render("~/https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css")
    <!-- Tempusdominus Bbootstrap 4 -->
    @Styles.Render("~/AdminLTE3-master/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css")
    <!-- iCheck -->
    @Styles.Render("~/AdminLTE3-master/plugins/icheck-bootstrap/icheck-bootstrap.min.css")
    <!-- JQVMap -->
    @Styles.Render("~/AdminLTE3-master/plugins/jqvmap/jqvmap.min.css")
    <!-- Theme style -->
    @Styles.Render("~/AdminLTE3-master/dist/css/adminlte.min.css")
    <!-- overlayScrollbars -->
    @Styles.Render("~/AdminLTE3-master/plugins/overlayScrollbars/css/OverlayScrollbars.min.css")
    <!-- Daterange picker -->
    @Styles.Render("~/AdminLTE3-master/plugins/daterangepicker/daterangepicker.css")
    <!-- summernote -->
    @Styles.Render("~/AdminLTE3-master/plugins/summernote/summernote-bs4.css")
    <!-- Google Font: Source Sans Pro -->
    @Styles.Render("~/https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700")
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
</head>
<body class="sidebar-mini sidebar-collapse" style="height: auto;">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="@Url.Action("Index", "Admin")" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="@Url.Action("Contact", "Admin")" class="nav-link">Contact</a>
                </li>
            </ul>

            <!-- SEARCH FORM -->
            <form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="#" class="brand-link">
                <img src="@Url.Content("~/AdminLTE3-master/dist/img/AdminLTELogo.png")" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Admin Menu</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@Url.Content("~/AdminLTE3-master/dist/img/user2-160x160.jpg")" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Admin UserName</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                    with font-awesome or any other icon font library -->
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Admin")" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>Dashboard</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("AccountTable", "Admin")" class="nav-link">
                                <i class="bi bi-person-fill-gear"></i>
                                <p>Account Table</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("PositionManager", "Admin")" class="nav-link">
                                <i class="bi bi-briefcase-fill"></i>
                                <p>Position Manager</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("OfficeManager", "Admin")" class="nav-link">
                                <i class="bi bi-building-fill-gear"></i>
                                <p>Office Manager</p>
                            </a>
                        </li>

                    </ul>

                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- jQuery -->
        @Scripts.Render("~/AdminLTE3-master/plugins/jquery/jquery.min.js")
        <!-- /.JQuery-->
        <!-- Content Wrapper. Contains page content -->
        <section class="content">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </section>


        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2014-2019 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.0.1-pre
            </div>
        </footer>
    </div>


    <!--Data Mana-->
    @Scripts.Render("~/Scripts/DataManager.js")
    <!-- jQuery UI 1.11.4 -->
    @Scripts.Render("~/AdminLTE3-master/plugins/jquery-ui/jquery-ui.min.js")
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    @Scripts.Render("~/AdminLTE3-master/plugins/bootstrap/js/bootstrap.bundle.min.js")
    <!-- ChartJS -->
    @Scripts.Render("~/AdminLTE3-master/plugins/chart.js/Chart.min.js")
    <!-- Sparkline -->
    @Scripts.Render("~/AdminLTE3-master/plugins/sparklines/sparkline.js")
    <!-- JQVMap -->
    @Scripts.Render("~/AdminLTE3-master/plugins/jqvmap/jquery.vmap.min.js")
    @Scripts.Render("~/AdminLTE3-master/plugins/jqvmap/maps/jquery.vmap.usa.js")
    <!-- jQuery Knob Chart -->
    @Scripts.Render("~/AdminLTE3-master/plugins/jquery-knob/jquery.knob.min.js")
    <!-- daterangepicker -->
    @Scripts.Render("~/AdminLTE3-master/plugins/moment/moment.min.js")
    @Scripts.Render("~/AdminLTE3-master/plugins/daterangepicker/daterangepicker.js")
    <!-- Tempusdominus Bootstrap 4 -->
    @Scripts.Render("~/AdminLTE3-master/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js")
    <!-- Summernote -->
    @Scripts.Render("~/AdminLTE3-master/plugins/summernote/summernote-bs4.min.js")
    <!-- overlayScrollbars -->
    @Scripts.Render("~/AdminLTE3-master/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js")
    <!-- AdminLTE App -->
    @Scripts.Render("~/AdminLTE3-master/dist/js/adminlte.js")
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @Scripts.Render("~/AdminLTE3-master/dist/js/pages/dashboard.js")
    <!-- AdminLTE for demo purposes -->
    @Scripts.Render("~/AdminLTE3-master/dist/js/demo.js")
</body>
</html>
